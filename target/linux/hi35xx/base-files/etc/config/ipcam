config ipcam 'global'
	option vendor	'zftlab'
	option model	'openipc'
	option soc	'hi3518ev200'
	option totmem	'64M'
	option sysmem	'40M'
	option flash	'8M'

config ipcam 'video'
	option sensor	'ar0130'
	option disabled	'0'

config ipcam 'gpio'
	option ircut1	'64'
	option ircut2	'65'
	option light	'47'
	option irled	'-'
	option wifi	'-'
	option audio	'-'
	option led1	'-'
	option reset	'-'

config ipcam 'i2c'
	option status	'-'

config service 'sdcard'
	option enabled '1'
	option photo '1'
	option video '1'
	option overwrite '1'
